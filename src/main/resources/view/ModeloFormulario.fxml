                        <TableColumn fx:id="colClienteContrato" prefWidth="150.0" text="Cliente" />
                        <TableColumn fx:id="colUsuarioCriadorContrato" prefWidth="100.0" text="Usuário" />
                        <TableColumn fx:id="colDataContrato" prefWidth="100.0" text="Data" />
                        <TableColumn fx:id="colStatusContrato" prefWidth="100.0" text="Status" />
