            <VBox fx:id="containerContratos" visible="false" managed="false" spacing="15.0">
                <Label text="Cadastro e Listagem de Contratos de Locação" style="-fx-font-size: 16pt;" />
                <GridPane hgap="10.0" vgap="10.0">
                    <Label text="ID:" GridPane.rowIndex="0" />
                    <TextField fx:id="txtIdContrato" editable="false" promptText="Gerado Automaticamente" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                    <Label text="Cliente:" GridPane.rowIndex="1" />
                    <ComboBox fx:id="cmbClienteContrato" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <Label text="Usuário Criador:" GridPane.rowIndex="2" />
                    <ComboBox fx:id="cmbUsuarioCriadorContrato" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
